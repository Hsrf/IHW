module MuxPCSource(A, B, C, D, E, F, G, out, PCSource);

	input logic [31:0] A, D, E, G;
	input logic B, C, F;
	input logic [2:0] PCSource;
	output logic [31:0] out;
  
    always @(PCSource)begin
      case(PCSource)
            0: out = A;
            1: out = B;
            2: out = C;
            3: out = D;
            4: out = E;
            5: out = F;
            6: out = G;
        endcase
    end
endmodule